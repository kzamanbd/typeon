{
    "name": "typeon",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/vite": "^4.1.13",
        "@types/react-router-dom": "^5.3.3",
        "clsx": "^2.1.1",
        "lucide-react": "^0.544.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-router-dom": "^7.9.1",
        "tailwindcss": "^4.1.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@types/node": "^24.4.0",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react-swc": "^4.0.1",
        "eslint": "^9.35.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.4.0",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.44.0",
        "vite": "^7.1.5"
    },
    "prettier": {
        "semi": true,
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "insertPragma": false,
        "bracketSameLine": true,
        "jsxSingleQuote": true,
        "printWidth": 120,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "requirePragma": false,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all",
        "useTabs": false,
        "endOfLine": "lf",
        "plugins": [
            "prettier-plugin-tailwindcss"
        ]
    }
}
